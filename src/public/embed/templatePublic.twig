<!doctype html>
<html lang="ru">
	<head>
		<meta charset="utf-8">
		<meta
		name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

		<!-- JS Scripts/Plugins-->
		<script src="{{ CONFIG.ROOTURL }}/static-assets/vendor/jquery/jquery-3.6.0.min.js"></script>
		<script src="{{ CONFIG.ROOTURL }}/static-assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
		<link rel="stylesheet" href="{{ CONFIG.ROOTURL }}/static-assets/vendor/bootstrap/css/bootstrap.min.css"/>
		<script src="{{ CONFIG.ROOTURL }}/static-assets/vendor/bootbox/bootbox.min.js"></script>
		<script src="{{ CONFIG.ROOTURL }}/static-assets/vendor/sweetalert2/sweetalert2.min.js"></script>
		<link rel="stylesheet" href="{{ CONFIG.ROOTURL }}/static-assets/libs/sweetalert2/sweetAlert2-bootstrap-4.min.css"/>
		<script src="{{ CONFIG.ROOTURL }}/static-assets/vendor/moment/moment.min.js"></script>
		<link rel="stylesheet" href="{{ CONFIG.ROOTURL }}/static-assets/vendor/daterangepicker/css/daterangepicker.min.css"/>
		<script src="{{ CONFIG.ROOTURL }}/static-assets/vendor/daterangepicker/js/daterangepicker.min.js"></script>
		<link rel="stylesheet" href="{{ CONFIG.ROOTURL }}/static-assets/vendor/fontawesome/css/all.min.css"/>
		<script src="{{ CONFIG.ROOTURL }}/static-assets/vendor/select2/js/select2.full.min.js"></script>
		<link rel="stylesheet" href="{{ CONFIG.ROOTURL }}/static-assets/vendor/select2/css/select2.min.css"/>
		<link rel="stylesheet" href="{{ CONFIG.ROOTURL }}/static-assets/libs/select2-bootstrap4/select2-bootstrap4.min.css"/>
		<link rel="stylesheet" href="{{ CONFIG.ROOTURL }}/static-assets/vendor/slick/css/slick.min.css"/>
		<link rel="stylesheet" href="{{ CONFIG.ROOTURL }}/static-assets/vendor/slick/css/slick-theme.min.css"/>
		<script src="{{ CONFIG.ROOTURL }}/static-assets/vendor/slick/js/slick.min.js"></script>
		<style>
			.select2-selection--multiple {
				max-height: 10rem;
				overflow-y: auto
			}
		</style>

		<style>
			html {
				position: relative;
				min-height: 100%;
			}
			body {
				font-size: 0.875rem;
			}
			.h3,
			h3 {
				font-size: 1rem;
			}
			[role="main"] {
				padding: 10px;
			}
		</style>
	</head>

	<body>
		<div class="container-fluid">
			<div class="row">
				<main role="main" class="col-sm-12 col-md-12 col-lg-12 col-xl-12 p-0">
					<div class="px-3 py-0 h-100"> {% block main %}{% endblock %}
						</div>
					</main>
				</div>
			</div>



			{{ CONFIG.FOOTER_ANALYTICS|raw }}
		</body>
	</html>
